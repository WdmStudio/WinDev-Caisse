            "texts": {
                "type": "CATEGORY",
                "label": "Textes",
                "payment": {
                    "type": "stChaine",
                    "edit": "multiline",
                    "label": "Chaine de réglement",
                    "isDynamic": false,
                    "defaut": "<linedisplay>\n\t<line>\n\t\t<text length=\"adapt\">{detail.ProductName}</text>\n\t\t<text align=\"right\">{detail.TTCPrice|ToPrice}Eur</text>\n\t</line>\n\t<line>\n\t\t<text length=\"adapt\">TOTAL</text>\n\t\t<text align=\"right\">{toPayedPrice|ToPrice}Eur</text>\n\t</line>\n</linedisplay>"
                },
                "productunit": {
                    "type": "stChaine",
                    "edit": "multiline",
                    "label": "Chaine de produit à l'unité",
                    "isDynamic": false,
                    "defaut": "<linedisplay>\n\t<line>\n\t\t<text length=\"adapt\">{detail.ProductName}</text>\n\t\t<text align=\"right\">{detail.UnitTTCPrice|ToPrice}Eur</text>\n\t</line>\n\t<line>\n\t\t<text length=\"adapt\">x {detail.Quantity}</text>\n\t\t<text align=\"right\">{detail.TTCPrice|ToPrice}Eur</text>\n\t</line>\n</linedisplay>"
                },
                "productweight": {
                    "type": "stChaine",
                    "edit": "multiline",
                    "label": "Chaine de produit au poids",
                    "isDynamic": false,
                    "defaut": "<linedisplay>\n\t<line>\n\t\t<text length=\"adapt\">{detail.ProductName}</text>\n\t\t<text align=\"right\">{detail.UnitTTCPrice|ToPrice}Eur</text>\n\t</line>\n\t<line>\n\t\t<text length=\"adapt\">x {detail.Quantity|ToWeight}</text>\n\t\t<text align=\"right\">{detail.TTCPrice|ToPrice}Eur</text>\n\t</line>\n</linedisplay>"
                },
                "info": {
                    "type": "stChaine",
                    "edit": "multiline",
                    "label": "Chaine d'info",
                    "isDynamic": false,
                    "defaut": "<linedisplay>\n\t<line>\n\t\t<text>{message}</text>\n\t</line>\n\t<line>\n\t\t<text align=\"right\">{value|toPrice}Eur</text>\n\t</line>\n</linedisplay>"
                },
                "idle": {
                    "type": "stChaine",
                    "edit": "multiline",
                    "label": "Chaine d'attente client",
                    "isDynamic": false,
                    "defaut": "<linedisplay>\n\t<line>\n\t\t<text align=\"center\">WDMStudio</text>\n\t</line>\n\t<line>\n\t\t<text align=\"center\">Bienvenue</text>\n\t</line>\n</linedisplay>"
